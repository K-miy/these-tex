@TechReport{TW.99,
  title = {The Weapon-Target Assignment Problem},
  author = {Alexander Toet and Huub Waard},
  institution =  "CALMA",
  year = {1995},
  abstract = "Various combinatorial optimization techniques are currently available. Most of these
techniques have not been thoroughly tested on realistic problems. The EUCLID (EUropean Cooperation
for the Long term In Defence)CALMA (Combinatorial Algorithms for
Military Applications) RTP (Research and Technology Project) 6.4 project has the main objective
to investigate the relevance of various existing algorithmic optimization techniques
to the actual solution of complex combinatorial problems arising in military applications.
This report briefly describes the weapon-threat assignment problem. This is a special
case of the more general optimal resource allocation problem, which is an actual real-time
defence industry problem with a large number of military applications. It is indicated
how an appropriate simplified simulation model can be constructed. A previous study, that
solved the problem using a genetic algorithm, is briefly reviewed. No attempt is made to
solve the problem here. However, it is suggested that this problem is a good candidate
for the testbed that is required for the last part of the CALMA study, which will be a
comparative evaluation of the different optimization techniques that were studied and/or
developed in the earlier stages of this project. The outcome of this comparative study may
lead to a better understanding of the applicability of these methods.
",
  annote = "Summarized description of the generalized weapon target assignment problem. No more, no less."
}

